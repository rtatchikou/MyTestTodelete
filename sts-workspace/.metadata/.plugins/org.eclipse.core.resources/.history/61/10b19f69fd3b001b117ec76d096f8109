package com.lyncwork.services.gusto.repository;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.lyncwork.services.expensify.domain.UserIntegrationQueue;
import com.lyncwork.services.expensify.domain.UserIntegrationStatus;

import java.util.List;


/**
 * Spring Data  repository for the UserIntegrationQueue entity.
 */

@Repository
public interface UserIntegrationQueueRepository extends JpaRepository<UserIntegrationQueue, Long> {

    List<UserIntegrationQueue> findAllByConsumerIgnoreCaseAndStatus(String consumer, UserIntegrationStatus status);
}
