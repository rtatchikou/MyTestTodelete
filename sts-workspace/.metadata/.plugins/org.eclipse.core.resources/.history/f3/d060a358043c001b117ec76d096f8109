package com.lyncwork.services.expensify.client.store.config;

import com.lyncwork.services.expensify.client.expensify.interceptor.GustoClientInterceptor;
import com.lyncwork.services.expensify.client.store.interceptor.StoreClientInterceptor;

import org.springframework.context.annotation.Bean;

public class StoreConfig {

    @Bean
    public StoreClientInterceptor storeClientInterceptor (){
        StoreClientInterceptor storeClientInterceptor =new StoreClientInterceptor();
        return storeClientInterceptor;
    }
}
