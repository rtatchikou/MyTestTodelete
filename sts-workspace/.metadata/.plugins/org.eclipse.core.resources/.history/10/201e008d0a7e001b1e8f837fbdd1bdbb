	public void deleteMember(DeleteMemberRequest request) {
<span class="nc" id="L42">		DeleteResponse deleteResponse = dropboxMemberClientFeign.deleteMember(request);</span>
<span class="nc" id="L43">		log.info(&quot;Delete member : {}&quot;, deleteResponse);</span>
<span class="nc" id="L44">	}</span>


	@Override
	public void recoverMember(RecoverMemberRequest request) {
<span class="nc" id="L49">		RecoverMemberErrorResponse err = dropboxMemberClientFeign.recoverMember(request);</span>
<span class="nc bnc" id="L50" title="All 2 branches missed.">		if(err != null) {</span>
<span class="nc" id="L51">			log.warn(&quot;Recover member err response : {}&quot;, err);</span>
		} else {
<span class="nc" id="L53">			log.info(&quot;User {} has been recovered. Please check the dropbox console.&quot;, request.getUser().getTeamMemberId());</span>
		}
		
<span class="nc" id="L56">	}</span>

}
