	public void getMembers(int limit, boolean includeRemoved) {
<span class="nc" id="L35">		ListMember members = dropboxMemberClientFeign.getMembers(new ListMemberRequest(limit, includeRemoved));</span>
<span class="nc" id="L36">		log.info(&quot;List members : {}&quot;, members);</span>
<span class="nc" id="L37">	}</span>


