package com.efunzo.be.training.utils;

import com.efunzo.be.training.domaine.Address;
import com.efunzo.be.training.domaine.User;
import com.fasterxml.jackson.core.JsonProcessingException;
import com.fasterxml.jackson.databind.ObjectMapper;

public class TestUtils {

	

	
	public static void main(String[] args) throws JsonProcessingException {
		ObjectMapper mapper = new ObjectMapper();
		
		User user = new User();
		user.setFistName("Jean");
		user.setLastName("Doe");
		
		String userJson = mapper.writeValueAsString(user);
		
		System.out.println(userJson);
		
		
		
		Address address = new Address();
		address.setLineOne("1 Mississauga Road");
		address.setPostalCode("LzL 4Z7");
		address.setCity("Toronto");
		address.setCountry("Canada");
		
		String addressJson = mapper.writeValueAsString(address);
		
		System.out.println(addressJson);
	}
}
