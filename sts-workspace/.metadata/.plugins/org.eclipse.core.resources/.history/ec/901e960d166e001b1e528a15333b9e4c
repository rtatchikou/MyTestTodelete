package com.lyncwork.services.dropbox.client.dropbox.model.request.member.add;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonProperty;

import java.util.ArrayList;
import java.util.List;

@JsonInclude(JsonInclude.Include.NON_NULL)
public class AddMemberRequest {
	@JsonProperty("new_members")
	private List<AddMember> newMembers = new ArrayList<>();
	
	@JsonProperty("force_async")
	private boolean forceAsync;

	public List<AddMember> getNewMembers() {
		return newMembers;
	}

	public void setNewMembers(List<AddMember> newMembers) {
		this.newMembers = newMembers;
	}

	public boolean isForceAsync() {
		return forceAsync;
	}

	public void setForceAsync(boolean forceAsync) {
		this.forceAsync = forceAsync;
	}

	@Override
	public String toString() {
		return "AddMemberRequest{" +
				"newMembers=" + newMembers +
				", forceAsync=" + forceAsync +
				'}';
	}
}
