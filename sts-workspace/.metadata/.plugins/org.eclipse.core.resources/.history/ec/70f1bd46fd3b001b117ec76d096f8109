package com.lyncwork.services.gusto.repository;

import com.lyncwork.services.gusto.domain.UserIntegrationMessage;
import com.lyncwork.services.gusto.domain.UserIntegrationStatus;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import java.util.List;


/**
 * Spring Data  repository for the UserIntegrationMessage entity.
 */
@SuppressWarnings("unused")
@Repository
public interface UserIntegrationMessageRepository extends JpaRepository<UserIntegrationMessage, Long> {

    List<UserIntegrationMessage> findAllByStatus(UserIntegrationStatus status);
}
