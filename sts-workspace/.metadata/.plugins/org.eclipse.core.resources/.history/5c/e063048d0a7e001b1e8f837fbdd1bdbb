    @Column(name = &quot;sys_creation_date&quot;, nullable = false, updatable = false)
    @JsonIgnore
<span class="nc" id="L35">    private Instant sysCreationDate = Instant.now();</span>

<span class="nc" id="L37">    @LastModifiedDate</span>
    @Column(name = &quot;sys_update_date&quot;)
    @JsonIgnore
<span class="nc" id="L40">    private Instant sysUpdateDate = Instant.now();</span>

    @LastModifiedBy
    @Column(name = &quot;last_updated_by&quot;)
    @JsonIgnore
    private String lastUpdatedBy;

    public String getCreatedBy() {
<span class="nc" id="L48">        return createdBy;</span>
    }

