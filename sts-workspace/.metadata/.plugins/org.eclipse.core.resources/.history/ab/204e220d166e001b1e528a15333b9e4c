package com.lyncwork.services.dropbox.repository;


import com.lyncwork.services.dropbox.domain.Company;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;


/**
 * Spring Data  repository for the Company entity.
 */
@SuppressWarnings("unused")
@Repository
public interface CompanyRepository extends JpaRepository<Company, Long> {

   boolean existsByTenantId(String tenantId);

   Company findByTenantId(String tenantId);

   Company findByTenantIdAndCompanyName(String tenantId, String companyName);
}
