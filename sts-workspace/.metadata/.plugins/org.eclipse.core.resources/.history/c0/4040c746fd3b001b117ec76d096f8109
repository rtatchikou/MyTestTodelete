package com.lyncwork.services.gusto.domain;

import org.hibernate.annotations.Cache;
import org.hibernate.annotations.CacheConcurrencyStrategy;

import javax.persistence.*;
import java.io.Serializable;

/**
 * A UserIntegrationQueue.
 */
@Entity
@Table(name = "user_integration_queue")
@Cache(usage = CacheConcurrencyStrategy.NONSTRICT_READ_WRITE)
public class UserIntegrationQueue extends AbstractAuditingEntity implements Serializable {

    private static final long serialVersionUID = 1L;

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    @Column(name = "consumer")
    private String consumer;

    @ManyToOne
    @JoinColumn(name = "user_integration_message_id")
    private UserIntegrationMessage message;

    @Enumerated(EnumType.STRING)
    @Column(name = "status")
    private UserIntegrationStatus status;

    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (!(o instanceof UserIntegrationQueue)) {
            return false;
        }
        return id != null && id.equals(((UserIntegrationQueue) o).id);
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getConsumer() {
        return consumer;
    }

    public void setConsumer(String consumer) {
        this.consumer = consumer;
    }

    public UserIntegrationMessage getMessage() {
        return message;
    }

    public void setMessage(UserIntegrationMessage message) {
        this.message = message;
    }

    public UserIntegrationStatus getStatus() {
        return status;
    }

    public void setStatus(UserIntegrationStatus status) {
        this.status = status;
    }

    @Override
    public int hashCode() {
        return 31;
    }

    @Override
    public String toString() {
        return "UserIntegrationMessage{" +
            "id=" + getId() +
            ", consumer='" + getConsumer() + "'" +
            ", status='" + getStatus() + "'" +
            ", createdBy='" + getCreatedBy() + "'" +
            ", sysCreationDate='" + getSysCreationDate() + "'" +
            ", sysUpdateDate='" + getSysUpdateDate() + "'" +
            ", lastUpdatedBy='" + getLastUpdatedBy() + "'" +
            "}";
    }
}
