package com.lyncwork.services.dropbox.client.dropbox.model.request.group.list;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.fasterxml.jackson.annotation.JsonProperty;
import com.lyncwork.services.dropbox.client.dropbox.model.request.group.add.CreateGroupRequest;
import com.lyncwork.services.dropbox.client.dropbox.model.request.member.MemberRequest;

import java.util.List;

@JsonInclude(Include.NON_NULL)

public class BackupRequest {
	
	@JsonProperty("group")
	private CreateGroupRequest group;
	
	@JsonProperty("members")
	private List<MemberRequest> members;

	@JsonProperty("return_members")
	private boolean returnMembers;
}
