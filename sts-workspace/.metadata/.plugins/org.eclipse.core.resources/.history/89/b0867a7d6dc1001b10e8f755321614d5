package com.efunzo.be.training;



public class Factoriel {
	
	public static void main(String[] args) {
		
		int theFatorielOf= 6;
		
		int fact = 1;
		
		for (int i = theFatorielOf; i >=1; i--) {
			
			fact *= i;
			
		}
				
			System.out.println("Factoriel of "+theFatorielOf +" is: "+ fact);	
		
	}

}
