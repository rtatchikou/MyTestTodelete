package xyz.pakwo.expensify.domain;

import lombok.Getter;
import lombok.ToString;

@Getter
@ToString
public class GetInputSetting implements InputSetting {
	private String type;
	private boolean adminOnly;
	private String userEmail;

	public GetInputSetting(String type, boolean adminOnly, String userEmail) {
		this.type = type;
		this.adminOnly = adminOnly;
		this.userEmail = userEmail;
	}
	
	public GetInputSetting(String type) {
		this.type = type;
		
	}
}
