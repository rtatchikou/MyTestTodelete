package com.lyncwork.services.dropbox.client.dropbox.model.request.teamfolder.create;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonProperty;


@JsonInclude(JsonInclude.Include.NON_NULL)
public class FolderMemberTagRequest {

	@JsonProperty(".tag")
	private String tag;
	
	@JsonProperty("dropbox_id")
	private String dropboxId;

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}

	public String getDropboxId() {
		return dropboxId;
	}

	public void setDropboxId(String dropboxId) {
		this.dropboxId = dropboxId;
	}

	@Override
	public String toString() {
		return "FolderMemberTagRequest{" +
				"tag='" + tag + '\'' +
				", dropboxId='" + dropboxId + '\'' +
				'}';
	}
}

