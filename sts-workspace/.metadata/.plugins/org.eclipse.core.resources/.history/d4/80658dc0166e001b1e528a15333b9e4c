package com.lyncwork.services.hellosign.client.dropbox.service;


import com.amazonaws.services.managedblockchain.model.DeleteMemberRequest;
import com.lyncwork.services.hellosign.client.dropbox.model.request.member.add.AddMemberRequest;
import com.lyncwork.services.hellosign.client.dropbox.model.request.member.recover.RecoverMemberRequest;

public interface DropboxMemberService {
	
	void addMember(AddMemberRequest addMember);

	void getMembers(int limit, boolean includeRemoved);

	void deleteMember(DeleteMemberRequest request);

	void recoverMember(RecoverMemberRequest request);
	
}
