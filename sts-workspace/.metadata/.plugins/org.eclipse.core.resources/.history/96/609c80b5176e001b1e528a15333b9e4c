package com.lyncwork.services.hellosign.client.dropbox.model.response;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

import java.util.List;

@Setter
@Getter
@ToString
@JsonInclude(Include.NON_NULL)
public class SyncSetting {
	
	@JsonProperty(".tag")
	private String tag;

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}


	@JsonInclude(Include.NON_NULL)
	public static class TeamFolder {

		@JsonProperty("team_folder_id")
		private String teamFolderId;

		@JsonProperty("name")
		private String name;

		@JsonProperty("status")
		private Status status;

		@JsonProperty("is_team_shared_dropbox")
		private boolean isTeamSharedDropbox;

		@JsonProperty("sync_setting")
		private SyncSetting syncSetting;

		@JsonProperty("content_sync_settings")
		private List<ContentSyncSetting> contentSyncSettings;

		public String getTeamFolderId() {
			return teamFolderId;
		}

		public void setTeamFolderId(String teamFolderId) {
			this.teamFolderId = teamFolderId;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

		public Status getStatus() {
			return status;
		}

		public void setStatus(Status status) {
			this.status = status;
		}

		public boolean isTeamSharedDropbox() {
			return isTeamSharedDropbox;
		}

		public void setTeamSharedDropbox(boolean teamSharedDropbox) {
			isTeamSharedDropbox = teamSharedDropbox;
		}

		public SyncSetting getSyncSetting() {
			return syncSetting;
		}

		public void setSyncSetting(SyncSetting syncSetting) {
			this.syncSetting = syncSetting;
		}

		public List<ContentSyncSetting> getContentSyncSettings() {
			return contentSyncSettings;
		}

		public void setContentSyncSettings(List<ContentSyncSetting> contentSyncSettings) {
			this.contentSyncSettings = contentSyncSettings;
		}
	}
}