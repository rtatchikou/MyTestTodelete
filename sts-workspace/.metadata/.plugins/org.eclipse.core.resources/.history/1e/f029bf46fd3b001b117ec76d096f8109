package com.lyncwork.services.gusto.service.dto;


import com.lyncwork.services.gusto.domain.UserIntegrationQueue;
import com.lyncwork.services.gusto.domain.UserIntegrationStatus;

import java.io.Serializable;

/**
 * A DTO for the {@link UserIntegrationQueue} entity.
 */
public class UserIntegrationQueueDTO implements Serializable {

    private static final long serialVersionUID = 1L;

    private Long id;

    private String consumer;

    private UserIntegrationMessageDTO message;

    private UserIntegrationStatus status;

    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (!(o instanceof UserIntegrationQueueDTO)) {
            return false;
        }
        return id != null && id.equals(((UserIntegrationQueueDTO) o).id);
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getConsumer() {
        return consumer;
    }

    public void setConsumer(String consumer) {
        this.consumer = consumer;
    }

    public UserIntegrationMessageDTO getMessage() {
        return message;
    }

    public void setMessage(UserIntegrationMessageDTO message) {
        this.message = message;
    }

    public UserIntegrationStatus getStatus() {
        return status;
    }

    public void setStatus(UserIntegrationStatus status) {
        this.status = status;
    }

    @Override
    public int hashCode() {
        return 31;
    }

    @Override
    public String toString() {
        return "UserIntegrationMessage{" +
            "id=" + getId() +
            ", consumer='" + getConsumer() + "'" +
            ", status='" + getStatus() + "'" +
            "}";
    }
}
