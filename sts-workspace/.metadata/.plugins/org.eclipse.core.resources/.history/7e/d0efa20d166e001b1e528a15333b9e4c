package com.lyncwork.services.dropbox.client.dropbox.model.request.teamfolder.create;

import com.fasterxml.jackson.annotation.JsonProperty;

import java.util.List;

public class AddFolderMemberRequest {

	@JsonProperty("shared_folder_id")
	private String sharedFolderId;
	
	@JsonProperty("members")
	private List<FolderMemberRequest> members;
	
	@JsonProperty("quiet")
	private boolean quiet;
	
	@JsonProperty("custom_message")
	private String customMessage;

	public String getSharedFolderId() {
		return sharedFolderId;
	}

	public void setSharedFolderId(String sharedFolderId) {
		this.sharedFolderId = sharedFolderId;
	}

	public List<FolderMemberRequest> getMembers() {
		return members;
	}

	public void setMembers(List<FolderMemberRequest> members) {
		this.members = members;
	}

	public boolean isQuiet() {
		return quiet;
	}

	public void setQuiet(boolean quiet) {
		this.quiet = quiet;
	}

	public String getCustomMessage() {
		return customMessage;
	}

	public void setCustomMessage(String customMessage) {
		this.customMessage = customMessage;
	}

	@Override
	public String toString() {
		return "AddFolderMemberRequest{" +
				"sharedFolderId='" + sharedFolderId + '\'' +
				", members=" + members +
				", quiet=" + quiet +
				", customMessage='" + customMessage + '\'' +
				'}';
	}
}
