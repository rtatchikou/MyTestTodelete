package com.lyncwork.services.dropbox.service.mapper;


import com.lyncwork.services.dropbox.domain.Company;
import com.lyncwork.services.dropbox.service.dto.CompanyDTO;
import org.mapstruct.Mapper;
import org.mapstruct.Mapping;

;

/**
 * Mapper for the entity {@link Company} and its DTO {@link CompanyDTO}.
 */
@Mapper(componentModel = "spring")
public interface CompanyMapper extends EntityMapper<CompanyDTO, Company> {


    @Override
    @Mapping(source = "tenantId", target = "tenantId")
    @Mapping(source = "companyName", target = "companyName")
    @Mapping(source = "teamFolderName", target = "teamFolderName")
    @Mapping(source = "teamFolderNameSpaceId", target = "teamFolderNameSpaceId")
    @Mapping(source = "perentSharedFolderId", target = "perentSharedFolderId")
    @Mapping(source = "folderId", target = "folderId")
    CompanyDTO toDto(Company entity);

    @Override
    @Mapping(source = "tenantId", target = "tenantId")
    @Mapping(source = "companyName", target = "companyName")
    @Mapping(source = "teamFolderName", target = "teamFolderName")
    @Mapping(source = "teamFolderNameSpaceId", target = "teamFolderNameSpaceId")
    @Mapping(source = "perentSharedFolderId", target = "perentSharedFolderId")
    @Mapping(source = "folderId", target = "folderId")
    Company toEntity(CompanyDTO dto);
}
