package com.lyncwork.services.hellosign.client.dropbox.model.request.member.add;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonProperty;


@JsonInclude(JsonInclude.Include.NON_NULL)
public class AddMember {
	@JsonProperty("member_email")
	private String memberEmail;
	
	@JsonProperty("member_given_name")
	private String memberGivenName;
	
	@JsonProperty("member_surname")
	private String memberSurname;
	
	@JsonProperty("member_external_id")
	private String memberExternalId;
	
	@JsonProperty("send_welcome_email")
	private boolean sendWelcomeEmail;
	
	@JsonProperty("role")
	private String role;

	public String getMemberEmail() {
		return memberEmail;
	}

	public void setMemberEmail(String memberEmail) {
		this.memberEmail = memberEmail;
	}

	public String getMemberGivenName() {
		return memberGivenName;
	}

	public void setMemberGivenName(String memberGivenName) {
		this.memberGivenName = memberGivenName;
	}

	public String getMemberSurname() {
		return memberSurname;
	}

	public void setMemberSurname(String memberSurname) {
		this.memberSurname = memberSurname;
	}

	public String getMemberExternalId() {
		return memberExternalId;
	}

	public void setMemberExternalId(String memberExternalId) {
		this.memberExternalId = memberExternalId;
	}

	public boolean isSendWelcomeEmail() {
		return sendWelcomeEmail;
	}

	public void setSendWelcomeEmail(boolean sendWelcomeEmail) {
		this.sendWelcomeEmail = sendWelcomeEmail;
	}

	public String getRole() {
		return role;
	}

	public void setRole(String role) {
		this.role = role;
	}

	@Override
	public String toString() {
		return "AddMember{" +
				"memberEmail='" + memberEmail + '\'' +
				", memberGivenName='" + memberGivenName + '\'' +
				", memberSurname='" + memberSurname + '\'' +
				", memberExternalId='" + memberExternalId + '\'' +
				", sendWelcomeEmail=" + sendWelcomeEmail +
				", role='" + role + '\'' +
				'}';
	}
}
