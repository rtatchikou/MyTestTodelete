package com.lyncwork.services.dropbox.client.dropbox.model.request.group.add;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.lyncwork.services.hellosign.client.dropbox.model.request.group.GroupMemberRequest;
import com.lyncwork.services.hellosign.client.dropbox.model.request.group.GroupRequest;
import com.fasterxml.jackson.annotation.JsonProperty;

import java.util.List;


@JsonInclude(Include.NON_NULL)
public class AddGroupMemberRequest {
	
	@JsonProperty("group")
	private GroupRequest group;
	
	@JsonProperty("members")
	private List<GroupMemberRequest> members;
	
	@JsonProperty("return_members")
	private boolean returnMembers;

	public GroupRequest getGroup() {
		return group;
	}

	public void setGroup(GroupRequest group) {
		this.group = group;
	}

	public List<GroupMemberRequest> getMembers() {
		return members;
	}

	public void setMembers(List<GroupMemberRequest> members) {
		this.members = members;
	}

	public boolean isReturnMembers() {
		return returnMembers;
	}

	public void setReturnMembers(boolean returnMembers) {
		this.returnMembers = returnMembers;
	}

	@Override
	public String toString() {
		return "AddGroupMemberRequest{" +
				"group=" + group +
				", members=" + members +
				", returnMembers=" + returnMembers +
				'}';
	}
}
