package com.lyncwork.services.dropbox.client.dropbox.model.response.member;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;
import com.lyncwork.services.hellosign.client.dropbox.model.response.Status;
import com.fasterxml.jackson.annotation.JsonProperty;

import java.util.List;

@JsonInclude(Include.NON_NULL)
public class Profile {

	@JsonProperty("team_member_id")
	private String teamMemberId;

	@JsonProperty("email")
	private String email;

	@JsonProperty("email_verified")
	private boolean emailVerified;

	@JsonProperty(".tag")
	private String tag;
	
	@JsonProperty("name")
	private Name name;
	
	@JsonProperty("membership_type")
	private MembershipType membershipType;
	
	@JsonProperty("groups")
	private List<String> groups;
	
	@JsonProperty("member_folder_id")
	private String memberFolderId;
	
	@JsonProperty("external_id")
	private String externalId;
	
	@JsonProperty("account_id")
	private String accountId;
	
	@JsonProperty("secondary_emails")
	private List<VerifiedEmail> secondaryEmails; 
	
	@JsonProperty("join_on")
	private String joinOn;
	
	@JsonProperty("profile_photo_url")
	private String profilePhotoUrl;

	@JsonProperty("status")
	private Status status;

	public String getTeamMemberId() {
		return teamMemberId;
	}

	public void setTeamMemberId(String teamMemberId) {
		this.teamMemberId = teamMemberId;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public boolean isEmailVerified() {
		return emailVerified;
	}

	public void setEmailVerified(boolean emailVerified) {
		this.emailVerified = emailVerified;
	}

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}

	public Name getName() {
		return name;
	}

	public void setName(Name name) {
		this.name = name;
	}

	public MembershipType getMembershipType() {
		return membershipType;
	}

	public void setMembershipType(MembershipType membershipType) {
		this.membershipType = membershipType;
	}

	public List<String> getGroups() {
		return groups;
	}

	public void setGroups(List<String> groups) {
		this.groups = groups;
	}

	public String getMemberFolderId() {
		return memberFolderId;
	}

	public void setMemberFolderId(String memberFolderId) {
		this.memberFolderId = memberFolderId;
	}

	public String getExternalId() {
		return externalId;
	}

	public void setExternalId(String externalId) {
		this.externalId = externalId;
	}

	public String getAccountId() {
		return accountId;
	}

	public void setAccountId(String accountId) {
		this.accountId = accountId;
	}

	public List<VerifiedEmail> getSecondaryEmails() {
		return secondaryEmails;
	}

	public void setSecondaryEmails(List<VerifiedEmail> secondaryEmails) {
		this.secondaryEmails = secondaryEmails;
	}

	public String getJoinOn() {
		return joinOn;
	}

	public void setJoinOn(String joinOn) {
		this.joinOn = joinOn;
	}

	public String getProfilePhotoUrl() {
		return profilePhotoUrl;
	}

	public void setProfilePhotoUrl(String profilePhotoUrl) {
		this.profilePhotoUrl = profilePhotoUrl;
	}

	public Status getStatus() {
		return status;
	}

	public void setStatus(Status status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "Profile{" +
				"teamMemberId='" + teamMemberId + '\'' +
				", email='" + email + '\'' +
				", emailVerified=" + emailVerified +
				", tag='" + tag + '\'' +
				", name=" + name +
				", membershipType=" + membershipType +
				", groups=" + groups +
				", memberFolderId='" + memberFolderId + '\'' +
				", externalId='" + externalId + '\'' +
				", accountId='" + accountId + '\'' +
				", secondaryEmails=" + secondaryEmails +
				", joinOn='" + joinOn + '\'' +
				", profilePhotoUrl='" + profilePhotoUrl + '\'' +
				", status=" + status +
				'}';
	}
}

